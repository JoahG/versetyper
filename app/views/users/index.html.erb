<% if current_user %>
	<%= current_user.login %> 
	<%= link_to "Log Out", log_out_path %>
	<%= link_to "New Verse", new_verse_path %>
	<br>
	<%= current_user.wpm %> Average WPM
	<ul>
		<% current_user.verses.each do |verse| %>
			<li><%= link_to verse.ref, type_path(verse) %></li>
		<% end %>
	</ul>
	<ul>
		<% current_user.verse_completions.each do |vc| %>
			<li><%= vc.verse.ref %> - <%= vc.wpm %> wpm</li>
		<% end %>
	</ul>
<%else%>
	<%= link_to "Log In", log_in_path %>
<%end%>